<FindBugsFilter>
    <Match>
        <!-- Do not fail on Spring REST endpoints. -->
        <Bug pattern="SPRING_ENDPOINT"/>
    </Match>
    <Match>
        <!-- Classes should generally use generated SerialVersionUID unless there is a reason to implement compatibility. -->
        <Bug pattern="SE_NO_SERIALVERSIONID"/>
    </Match>
    <Match>
        <!-- Filter generated sources.. -->
        <Package name="net.catenax.prs.client.api"/>
    </Match>
    <Match>
        <!-- Filter generated sources.. -->
        <Package name="net.catenax.prs.client"/>
    </Match>
    <Match>
        <!-- Filter generated sources.. -->
        <Package name="net.catenax.prs.client.auth"/>
    </Match>
</FindBugsFilter>
